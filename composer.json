{
	"name": "nette/forms",
	"description": "üìù Nette Forms: generating, validating and processing secure forms in PHP. Handy API, fully customizable, server & client side validation and mature design.",
	"keywords": ["nette", "forms", "validation", "csrf", "javascript", "bootstrap"],
	"homepage": "https://nette.org",
	"license": ["BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"authors": [
		{
			"name": "David Grudl",
			"homepage": "https://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "https://nette.org/contributors"
		}
	],
	"require": {
		"php": "8.2 - 8.5",
		"nette/component-model": "^3.2",
		"nette/http": "^3.3",
		"nette/utils": "^4.1"
	},
	"require-dev": {
		"nette/application": "^3.3",
		"nette/di": "^3.2",
		"nette/tester": "^2.6",
		"latte/latte": "^3.1",
		"tracy/tracy": "^2.11",
		"phpstan/phpstan": "^2.1@stable",
		"phpstan/extension-installer": "^1.4@stable",
		"nette/phpstan-rules": "^1.0"
	},
	"conflict": {
		"latte/latte": "<3.1 || >=3.2"
	},
	"suggest": {
		"ext-intl": "to use date/time controls"
	},
	"autoload": {
		"classmap": ["src/"],
		"psr-4": {
			"Nette\\": "src"
		}
	},
	"minimum-stability": "dev",
	"scripts": {
		"phpstan": "phpstan analyse",
		"tester": "tester tests -s"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.3-dev"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
